{
    "name": "flux/hidraulica-disseny",
    "description": "Hidraulica Disseny Symfony 2 Website",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "require": {
        "a2lix/translation-form-bundle": "1.*",
        "astina/social-links-bundle": "^2.2",
        "components/font-awesome": "^4.0",
        "components/jquery": "^2.0",
        "cypresslab/gitelephant-bundle": "dev-develop",
        "doctrine/data-fixtures": "1.1.1",
        "doctrine/doctrine-bundle": "~1.4",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^1.0",
        "doctrine/migrations": "^1.0",
        "doctrine/orm": "^2.4.8",
        "egulias/email-validator": "^1.2",
        "hautelook/alice-bundle": "^1.0",
        "jms/i18n-routing-bundle": "^2.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "ircmaxell/password-compat": "~1.0",
        "knplabs/knp-menu-bundle": "^2.0",
        "knplabs/knp-paginator-bundle": "^2.5",
        "liip/functional-test-bundle": "^1.0",
        "liip/imagine-bundle": "^1.0",
        "payum/payum-bundle": "^2.0",
        "payum/paypal-express-checkout-nvp": "^1.3",
        "payum/offline": "^1.3",
        "php": ">=5.3.9",
        "php-http/guzzle6-adapter": "^1.0",
        "presta/sitemap-bundle": "^1.3",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "sonata-project/admin-bundle": "^3.0",
        "sonata-project/doctrine-orm-admin-bundle": "^3.0",
        "sonata-project/user-bundle": "^3.0",
        "stfalcon/tinymce-bundle": "1.x-dev",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/assetic-bundle": "^2.7",
        "symfony/monolog-bundle": "~2.4",
        "symfony/symfony": "2.8.*",
        "symfony/swiftmailer-bundle": "~2.3",
        "twbs/bootstrap": "^3.0",
        "twig/extensions": "^1.3",
        "vich/uploader-bundle": "^0.14.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
